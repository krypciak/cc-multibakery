{
    "name": "cc-multibakery",
    "version": "0.5.1",
    "type": "module",
    "scripts": {
        "start": "bun build/build.ts build",
        "watch": "bun build/build.ts watch",
        "build": "bun build/build.ts build",
        "regenerateBin": "bun build/build.ts build forceRegenerateBinaryEncodeDecodeScripts=true",
        "buildRemote": "bun build/build.ts build minifySyntax=true minifyWhitespace=true physics=false extraTreeShaking=true",
        "buildRemoteBrowser": "bun build/build.ts build minifySyntax=true minifyWhitespace=true physics=false browser=true target=es2024 extraTreeShaking=true",
        "buildPhysics": "bun build/build.ts build minifySyntax=true minifyWhitespace=true remote=false extraTreeShaking=true",
        "buildPhysicsNetless": "bun build/build.ts build minifySyntax=true minifyWhitespace=true remote=false physicsnet=false extraTreeShaking=true",
        "format": "prettier ./src -w; npx prettier ./assets -w --tab-width 4 --no-semi --print-width 200 --bracket-same-line",
        "test": "NODE_NO_WARNINGS=1 node --enable-source-maps scripts/test.js",
        "server": "NODE_NO_WARNINGS=1 node --enable-source-maps scripts/server.js"
    },
    "devDependencies": {
        "@types/bun": "^1.2.22",
        "@types/http-server": "^0.12.4",
        "@types/jquery": "^3.5.32",
        "@types/node": "^24.3.0",
        "bun": "^1.2.20",
        "cc-bundler": "github:krypciak/cc-bundler",
        "cc-instanceinator": "github:krypciak/cc-instanceinator",
        "ccmoddb": "github:CCDirectLink/CCModDB",
        "ccmodmanager": "github:CCDirectLink/CCModManager",
        "crossnode": "github:krypciak/crossnode",
        "esbuild": "^0.25.9",
        "fflate": "^0.8.2",
        "http-server": "^14.1.1",
        "nax-ccuilib": "github:krypciak/nax-ccuilib",
        "node": "^24.6.0",
        "prettier": "3.6.2",
        "setimmediate": "^1.0.5",
        "socket.io": "^4.8.1",
        "socket.io-adapter": "^2.5.5",
        "socket.io-client": "^4.8.1",
        "spdy": "^4.0.2",
        "ts-binarifier": "github:krypciak/ts-binarifier",
        "typescript": "^5.9.2",
        "ultimate-crosscode-typedefs": "github:krypciak/ultimate-crosscode-typedefs"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "bun",
            "esbuild",
            "node"
        ]
    }
}
